{
  "config_version": "v3",
  "terraform_version": "1.3.0",
  "default_env": "rdev",
  "app": "edu-math-api-app",
  "default_compose_env_file": ".env.ecr",
  "slice_default_tag": "branch-main",
  "services": ["edu-math-api"],
  "environments": {
    "rdev": {
      "k8s": {
        "namespace": "edu-platform-rdev-happy-happy-env",
        "cluster_id": "edu-platform-rdev-eks",
        "auth_method": "eks",
        "context": "edu-platform-rdev-eks"
      },
      "aws_profile": "edu-platform-playground-poweruser",
      "secret_arn": "happy/env-edu-platform-rdev-config",
      "terraform_directory": ".happy/terraform/envs/rdev",
      "log_group_prefix": "/happy/edu-math-api/rdev",
      "task_launch_type": "k8s",
      "auto_run_migrations": true
    }
  },
  "tasks": {
    "migrate": [],
    "delete": []
  },
  "features": {
    "enable_happy_api_usage": true,
    "enable_ecr_auto_creation": true,
    "enable_dynamo_locking": true
  }
}
